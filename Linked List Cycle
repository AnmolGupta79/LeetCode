class Solution {
public:
    bool hasCycle(ListNode *head) {
      ListNode *slow=head;
      ListNode *fast=head;  
      while(NULL!=fast && NULL!= fast->next){
        fast=fast->next->next;
        slow=slow->next;
        if(slow==fast){
            return true;
        }
      }
      return false;
    }
};

class Solution{
public:
vector<int> twoSum(vector<int>& nums, int target){
    unordered_map<int,int>m;
    vector<int>ans;
    for(int i=0;i<nums.size();i++){
        int ft=nums[i];
        int sec=target-ft;
        if(m.find(sec)!=m.end()){
            ans.push_back(i);
            ans.push_back(m[sec]);
            break;
        }
        m[ft]=i;
    }
    return ans;
}
};

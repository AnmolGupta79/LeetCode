class Solution {
public:
    int numOfUnplacedFruits(vector<int>& f, vector<int>& b) {
        int n1=f.size();
        int c=0;
        for(int i=0;i<n1;i++){
            for(int j=0;j<n1;j++){
                if(f[i]<=b[j]){
                   c++;
                   b[j]=0;
                   break;     
                }
            }
        }
        return n1-c;
    }
};
